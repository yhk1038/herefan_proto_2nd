<div id="follow_list_box" class="pull-right hf-transition-200">
    <div class="follow_list_wrap">
        <a href="" class="follow_left follow_move hf-transition-150">
            <span class="zmdi zmdi-chevron-left"></span>
        </a>
        <% @my_published_fandoms.each do |fandom| %>
            <div class="follow_list hf-transition-200">
                <%= link_to fandom_path(fandom) do %>
                    <div class="follow_artists" style="background: url('<%= fandom.config.fd_logo %>');">
                        <span><%= fandom.config.fd_name %></span>
                    </div>
                <% end %>
            </div>
        <% end %>
        <a href="" class="follow_right follow_move hf-transition-150">
            <span class="zmdi zmdi-chevron-right"></span>
        </a>
    </div>
</div>